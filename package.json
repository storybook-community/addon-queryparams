{
  "name": "@storybook/addon-queryparams",
  "version": "9.0.1",
  "description": "Mock query params in Storybook",
  "keywords": [
    "storybook-addon",
    "storybook",
    "mock",
    "queryparams",
    "URLSearchParams"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/storybook-community/addon-queryparams"
  },
  "license": "MIT",
  "author": "Storybook",
  "type": "module",
  "imports": {
    "#*": [
      "./*",
      "./*.ts",
      "./*.tsx"
    ]
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./preview": {
      "types": "./dist/preview.d.ts",
      "default": "./dist/preview.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "*.js",
    "*.d.ts"
  ],
  "scripts": {
    "prebuild": "node -e \"fs.rmSync('./dist', { recursive: true, force: true })\"",
    "build": "tsup",
    "build-storybook": "storybook build",
    "build:watch": "npm run build -- --watch",
    "eject-ts": "zx scripts/eject-typescript.js",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "lint": "eslint --cache .",
    "lint:fix": "pnpm lint --fix",
    "prepare": "husky",
    "prerelease": "zx scripts/prepublish-checks.js",
    "release": "pnpm build && pnpm semantic-release",
    "release:canary": "pnpm build && auto canary",
    "start": "run-p build:watch \"storybook --quiet\"",
    "storybook": "storybook dev -p 6006",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^20.0.0",
    "@eslint/js": "^9.36.0",
    "@storybook/addon-docs": "next",
    "@storybook/icons": "^1.4.0",
    "@storybook/react-vite": "next",
    "@types/node": "^20",
    "@types/react": "^18.2.65",
    "@types/react-dom": "^18.2.21",
    "@vitejs/plugin-react": "^4.7.0",
    "auto": "^11.3.0",
    "boxen": "^8.0.1",
    "commitlint": "^20.0.0",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-storybook": "next",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.3",
    "npm-run-all2": "^8.0.4",
    "prettier": "^3.6.2",
    "prompts": "^2.4.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "semantic-release": "^24.2.9",
    "storybook": "next",
    "ts-dedent": "^2.2.0",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.44.1",
    "vite": "^7.0.5",
    "zx": "^8.7.1"
  },
  "peerDependencies": {
    "storybook": "^10"
  },
  "publishConfig": {
    "access": "public"
  },
  "bundler": {
    "managerEntries": [],
    "previewEntries": [
      "src/preview.ts",
      "src/index.ts"
    ],
    "nodeEntries": []
  },
  "storybook": {
    "displayName": "Query Params (community version)",
    "supportedFrameworks": [
      "supported-frameworks"
    ],
    "icon": "https://avatars.githubusercontent.com/u/216219414?s=200&v=4"
  },
  "packageManager": "pnpm@10.0.0",
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ],
    "overrides": {
      "@octokit/request-error@>=1.0.0 <5.1.1": ">=5.1.1",
      "@octokit/request@>=1.0.0 <8.4.1": ">=8.4.1",
      "@octokit/plugin-paginate-rest@>=1.0.0 <9.2.2": ">=9.2.2"
    }
  }
}
